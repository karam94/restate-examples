{
  "name": "@restatedev/examples-control-scheduling",
  "version": "0.1.0",
  "description": "A simple task scheduling example using Restate",
  "type": "commonjs",
  "license": "MIT",
  "author": "Restate developers",
  "email": "code@restate.dev",
  "scripts": {
    "build": "tsc --noEmitOnError",
    "format": "prettier --ignore-path .eslintignore --write \"**/*.+(js|ts|json)\"",
    "start": "ts-node-dev --transpile-only src/index.ts",
    "example-4": "ts-node-dev --transpile-only src/import_workflow.ts",
    "example-5": "ts-node-dev --transpile-only src/device_object.ts",
    "example-6": "ts-node-dev --transpile-only src/idle_workflow.ts",
    "all": "npm run example-4 && npm run example-5 && npm run example-6"
  },
  "dependencies": {
    "@restatedev/restate-sdk": "^1.5.4",
    "@restatedev/restate-sdk-clients": "^1.5.4"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2"
  }
}
